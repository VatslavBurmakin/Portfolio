  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "def best_model(model, params, features_train, target_train):\n",
    "    grid = GridSearchCV(model, param_grid = params, cv=2,\n",
    "                                            scoring='f1', n_jobs=-1)\n",
    "    grid.fit(features_train,target_train)\n",
    "    results = pd.DataFrame(grid.cv_results_).sort_values(by = 'rank_test_score')\n",
    "    \n",
    "    fit_time = results.iloc[0]['mean_fit_time']\n",
    "    predict_time = results.iloc[0]['mean_score_time']\n",
    "    return fit_time, predict_time, grid.best_score_ , grid.best_params_,"
   ]
  },
